{
  "manifest_version": 2,
  "name": "VRChatのインスタンスを固定IDで建てるブラウザ拡張",
  "version": "0.0.1",
  "description": "別にブラウザ拡張である必要ないんだけど、最近のブラウザはなんでもできて便利だね",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_popup": "option.html",
    "default_title": "VRChatのインスタンスを固定IDで建てるブラウザ拡張 - 設定画面"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "app.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
